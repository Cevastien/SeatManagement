          "name": "Get CSRF Token",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response has csrf_token\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('csrf_token');",
                  "    pm.collectionVariables.set(\"csrf_token\", jsonData.csrf_token);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/csrf-token",
              "host": ["{{base_url}}"],
              "path": ["api", "csrf-token"]
            },
            "description": "Retrieves a CSRF token for subsequent POST requests. The token is automatically saved to collection variables."
          },
          "response": [
            {
              "name": "Success",
